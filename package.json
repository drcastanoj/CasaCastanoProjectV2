{
  "name": "server",
  "version": "1.0.0",
  "description": "Server api",
  "author": "drcastanoj",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "start:app": "webpack-dev-server --config config/webpack.app.dev.js --inline -d --progress --port 5000 --open",
    "test:app": "karma start --test",
    "test:app:dev": "karma start --no-single-run ",
    "build:app": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build::app:dev": "webpack --config config/webpack.prod.js -d --progress --profile --bail",
    "lint": "tslint \"src/**/*.ts\"",
    "dev": "yarn start:dev && yarn start:app"
  },
  "dependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/typeorm": "^5.0.2",
    "fastify-formbody": "^2.0.0",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.7",
    "typescript": "^2.6.2",
    "@angular/animations": "~5.2.10",
    "@angular/common": "~5.2.10",
    "@angular/compiler": "~5.2.10",
    "@angular/core": "~5.2.10",
    "@angular/forms": "~5.2.10",
    "@angular/http": "~5.2.10",
    "@angular/platform-browser": "~5.2.10",
    "@angular/platform-browser-dynamic": "~5.2.10",
    "@angular/router": "~5.2.10",
    "bootstrap": "4.1.1",
    "core-js": "2.4.1",
    "rxjs": "6.1.0",
    "rxjs-compat": "^6.1.0",
    "systemjs": "0.21.3",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.0.0",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0",
    "@types/jasmine": "2.5.46",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "6.7.7",
    "awesome-typescript-loader": "3.1.2",
    "clean-webpack-plugin": "0.1.16",
    "cordova": "7.0.1",
    "css-loader": "0.27.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "node-sass": "^4.7.2",
    "null-loader": "0.1.1",
    "postcss": "5.2.16",
    "postcss-loader": "^1.3.3",
    "precss": "1.4.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.2.1",
    "rimraf": "2.6.1",
    "sass": "^1.0.0-beta.4",
    "sass-loader": "^6.0.6",
    "style-loader": "0.16.1",
    "to-string-loader": "^1.1.5",
    "tslint": "4.5.1",
    "tslint-loader": "3.4.3",
    "typescript": "2.5.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1",
    "webpack-merge": "4.1.1",
    "yargs": "^8.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
